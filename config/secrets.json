{
    "project_name": "Bank Bot",
    "environment": "production",
    "version": "1.0.0",
    "fallback_email": "arkadeep.int@gmail.com",
    "default_sender_name": "Bank Bot Support",
    "jwt": {
      "algorithm": "RS256",
      "secret_env_var": "JWT_SECRET_KEY",
      "access_token_expiry": 900,
      "refresh_token_expiry": 604800
    },
    "logging": {
      "level": "INFO",
      "log_to_file": true,
      "file_path": "logs/bankbot.log"
    },
    "security": {
      "password_salt_rounds": 12,
      "session_timeout_minutes": 30
    }
  }